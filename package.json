{
  "name": "@discord-bots/monorepo",
  "version": "1.0.0",
  "keywords": [
    "ethereum",
    "react",
    "discord",
    "workspaces",
    "yarn",
    "banklessDAO",
    "bankless"
  ],
  "private": true,
  "scripts": {
    "discord-bot:build": "yarn workspace @discord-bots/degen build",
    "discord-bot:start": "yarn workspace @discord-bots/degen start",
    "discord-bot:test": "yarn workspace @discord-bots/degen test",
    "build": "yarn workspace @discord-bots/degen build",
    "chain": "yarn workspace @discord-bots/hardhat chain",
    "fork": "yarn workspace @discord-bots/hardhat fork",
    "node": "yarn workspace @discord-bots/hardhat chain",
    "test": "yarn workspace @discord-bots/hardhat test",
    "start": "yarn workspace @discord-bots/degen start",
    "compile": "yarn workspace @discord-bots/hardhat compile",
    "deploy": "yarn workspace @discord-bots/hardhat deploy",
    "verify": "yarn workspace @discord-bots/hardhat etherscan-verify",
    "watch": "yarn workspace @discord-bots/hardhat watch",
    "accounts": "yarn workspace @discord-bots/hardhat accounts",
    "balance": "yarn workspace @discord-bots/hardhat balance",
    "send": "yarn workspace @discord-bots/hardhat send",
    "generate": "yarn workspace @discord-bots/hardhat generate",
    "account": "yarn workspace @discord-bots/hardhat account",
    "fundedwallet": "cd packages/hardhat && npx hardhat fundedwallet",
    "flatten": "cd packages/hardhat && npx hardhat flatten",
    "clean": "cd packages/hardhat && npx hardhat clean"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/discord-bot",
      "**/discord-bot/**",
      "**/hardhat",
      "**/hardhat/**",
      "**/express-server",
      "**/express-server/**"
    ]
  },
  "dependencies": {}
}