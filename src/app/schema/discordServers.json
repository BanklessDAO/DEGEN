{
  "$jsonSchema": {
    "bsonType": "object",
    "required": [
      "serverId",
      "name",
    ],
    "properties": {
      "serverId": {
        "bsonType": ["string"],
        "description": "Discord ID of the server."
      },
      "name": {
        "bsonType": ["string"],
        "description": "Discord server name"
      },
      "isDEGENSetup": {
        "bsonType": ["bool"],
        "description": "Flag that is true if the discord server has fully setup DEGEN",
      },
      "privateChannelId": {
        "bsonType": ["string"],
        "description": "The channel execution id of where private degen commands should be run"
      },
      "roles": {
        "bsonType": ["null", "object"],
        "description": "List of degen specific roles.",
        "properties": {
          "authorizedDegenId": {
            "bsonType": ["string"],
            "description": "Role for authorized degen organizers",
          }
        }
      }
    }
  }
}