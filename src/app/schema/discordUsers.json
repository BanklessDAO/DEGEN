{
  "$jsonSchema": {
    "bsonType": "object",
    "required": [
      "userId",
      "tag",
      "isPremium"
    ],
    "properties": {
      "userId": {
        "bsonType": ["string"],
        "description": "Discord Id of the user that is configured to use this command."
      },
      "tag": {
        "bsonType": ["string"],
        "description": "Discord handle of the user that is configured to use this command."
      },
      "reportedForPOAP": {
        "bsonType": ["number"],
        "description": "Number of times this user reported for POAP spam."
      },
      "isPremium": {
        "bsonType": ["boolean"],
        "description": "True if discord member is a premium DEGEN pro subscriber, false otherwise"
      },
      "walletSettings": {
        "bsonType": ["null", "array"],
        "description": "Wallet details.",
        "properties": {
          "ETH": {
            "bsonType": ["null", "array"],
            "description": "List of public addresses.",
            "minItems": 0,
            "uniqueItems": true,
            "items": {
              "bsonType": ["null", "object"],
              "description": "Eth address",
              "properties": {
                "publicAddress": {
                  "bsonType": ["null", "string"],
                  "uniqueItems": true,
                  "minItems": 0,
                  "description": "Eth address"
                },
                "isPOAPDeliveryEnabled": {
                  "bsonType": ["string", "null"],
                  "description": "Enabled Address"
                }
              }
            }
          },
          "MATIC": {
            "bsonType": ["null", "array"],
            "description": "List of public addresses and whether or not POAP is enabled",
            "minItems": 0,
            "uniqueItems": true,
            "items": {
              "bsonType": ["null", "object"],
              "description": "MATIC address",
              "properties": {
                "publicAddress": {
                  "bsonType": ["null", "string"],
                  "uniqueItems": true,
                  "minItems": 0,
                  "description": "MATIC address"
                },
                "isPOAPDeliveryEnabled": {
                  "bsonType": ["string", "null"],
                  "description": "Enabled Address"
                }
              }
            }
          }
        }
      }
    }
  }
}