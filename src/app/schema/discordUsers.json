{
  "$jsonSchema": {
    "bsonType": "object",
    "required": [
      "userId",
      "tag",
      "isPremium"
    ],
    "properties": {
      "userId": {
        "bsonType": ["string"],
        "description": "Discord Id of the user that is configured to use this command."
      },
      "tag": {
        "bsonType": ["string"],
        "description": "Discord handle of the user that is configured to use this command."
      },
      "reportedForPOAP": {
        "bsonType": ["number"],
        "description": "Number of times this user reported for POAP spam."
      },
      "isPremium": {
        "bsonType": ["boolean"],
        "description": "True if discord member is a premium DEGEN pro subscriber, false otherwise"
      },
      "connectedAddresses": {
        "bsonType": ["null", "array"],
        "description": "Wallet details.",
        "properties": {
          "adress": {
            "bsonType": ["null", "string"],
            "description": "Public addresses.",
            "minItems": 0,
            "uniqueItems": true
          },
          "chainId": {
            "bsonType": ["null", "string"],
            "description": "ChainlistId of the address.",
            "minItems": 0,
            "uniqueItems": true
          },
          "nickname": {
            "bsonType": ["null", "string"],
            "description": "An addresses nickname.",
            "minItems": 0,
            "uniqueItems": true
          }
      },
      "POAPAddress": {
        "bsonType": ["string"],
        "description": "Address DEGEN sends POAPs too"
        }
      }
    }
  }
}
